<!DOCTYPE html>
<html>
<head>
  <title>Admin - FunnyLand</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  
  <script src="https://kit.fontawesome.com/f9ea2f5791.js" crossorigin="anonymous"></script>
  <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>
<body>
  <%= render 'layouts/admin_navigation' %>
  <div class="container">
    <div class="row">
      <div class="mx-auto col-11 col-sm-10 col-lg-9 col-xl-8">
        <p id="notice"></p>
        <%= render 'layouts/flash_messages' %>
        <%= yield %>
      </div>
    </div>
  </div>
</body>
</html>